@model List<string>

<div>
    <div class="text-start">
        <h1 class="fw-bold">Heading</h1>
        <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Donec nec convallis sapien. Sed nibh velit, convallis ac ultricies sit amet, facilisis vitae felis. In malesuada quis elit a venenatis.</p>
    </div>
    <div class="row">
        @foreach (var imageUrl in Model)
        {
            <div class="col-md-6">
                <div class="card mb-3">
                    <div class="card-body">
                        <h5 class="card-title">Card title</h5>
                        <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                    </div>
                    <img src="@Url.Content(imageUrl)" class="car-img-top" alt="imagen" />
                </div>
            </div>
        }
    </div>
</div>
